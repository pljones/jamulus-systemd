[Unit]
Description=Jamulus Jazz Server List
Documentation=http://jamulus.drealm.info/jamulus
Documentation=https://github.com/corrados/jamulus/wiki/Server-Troubleshooting

[Install]
WantedBy=multi-user.target

[Service]
ExecStart=/opt/Jamulus/bin/run-Jamulus.sh Jazz
User=Jamulus
Group=Jamulus
Nice=-20
IOSchedulingClass=realtime
IOSchedulingPriority=3
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=Jamulus-Jazz
